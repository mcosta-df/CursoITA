import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class TesteCompra {

	@Test
	void compraAVista() {
		Compra c = new Compra(500);
		
		assertEquals(1,  c.getNumeroParcelas());
		assertEquals(500,  c.valorTotal);
		assertEquals(500,  c.getValorParcela());
		
	}

}
